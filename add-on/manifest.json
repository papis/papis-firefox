{

  "description": "Add paper to papis from current url.",
  "manifest_version": 2,
  "name": "Papis",
  "version": "0.1",
  "icons": {
    "32": "icons/paper.svg",
    "48": "icons/paper.svg",
    "64": "icons/paper.svg",
    "96": "icons/paper.svg"
  },

  "applications": {
    "gecko": {
      "id": "papis_connector@papis",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/paper.svg"
  },
  
  "permissions": [
    "nativeMessaging",
    "tabs"
    ]

}
